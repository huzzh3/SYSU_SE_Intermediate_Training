package dancingBug;

import info.gridworld.actor.Bug;

public class DancingBug {
	private int   steps;
    private int   sideLength;
    private int[] turnArray;
    
    public DancingBug(int length, int[] arrayInput)
    {
        steps = 0;
        sideLength = length;
        turnArray = arrayInput;
    }
}
